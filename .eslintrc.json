{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "import/no-extraneous-dependencies": [
            "error", {"devDependencies": true, "optionalDependencies": false, "peerDependencies": false}
        ],
        "no-underscore-dangle": [
            "error", { "allowAfterThis": true}
        ]
    }
}
